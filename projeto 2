#include <stdio.h>
#include <math.h>
float a,b;
int main()
{
    printf("Digite dois números:\n");
    scanf("%f %f",&a,&b);
    
    if (a>b){
    printf("O cubo do menor é %.2f e raiz quadrada do maior é %.2f\n",pow(b,3),sqrt(a));    
    }
    else{
    printf("O cubo do menor é %.2f e raiz quadrada do maior é %.2f\n",pow(a,3),sqrt(b));
    }
    return 0;
}